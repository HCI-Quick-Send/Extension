{
  "manifest_version": 2,

  "name": "Quick Send",
  "description": "This extension will allow you to share and recieve links from your friends",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Click here!"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
	"gcm",
	"storage",
	"notifications",
	"tabs",
    "https://ajax.googleapis.com/"
  ]
}