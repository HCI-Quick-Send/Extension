{
  "manifest_version": 2,

  "name": "QuickSend",
  "description": "This extension lets users share web content with one click",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "QuickSend"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
	"gcm",
	"storage",
    "https://ajax.googleapis.com/"
  ]
}